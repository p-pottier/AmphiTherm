@article{nowakowski_thermal_2018,
	title = {Thermal biology mediates responses of amphibians and reptiles to habitat modification},
	volume = {21},
	issn = {1461023X (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85040055542&doi=10.1111%2fele.12901&partnerID=40&md5=ea381ded8a7d882be5f3694d331d5c47},
	abstract = {Human activities often replace native forests with warmer, modified habitats that represent novel thermal environments for biodiversity. Reducing biodiversity loss hinges upon identifying which species are most sensitive to the environmental conditions that result from habitat modification. Drawing on case studies and a meta-analysis, we examined whether observed and modelled thermal traits, including heat tolerances, variation in body temperatures, and evaporative water loss, explained variation in sensitivity of ectotherms to habitat modification. Low heat tolerances of lizards and amphibians and high evaporative water loss of amphibians were associated with increased sensitivity to habitat modification, often explaining more variation than non-thermal traits. Heat tolerances alone explained 24–66\% (mean = 38\%) of the variation in species responses, and these trends were largely consistent across geographic locations and spatial scales. As habitat modification alters local microclimates, the thermal biology of species will likely play a key role in the reassembly of terrestrial communities. © 2018 John Wiley \& Sons Ltd/CNRS},
	language = {English},
	number = {3},
	journal = {Ecology Letters},
	author = {Nowakowski, A.J. and Watling, J.I. and Thompson, M.E. and Brusch, IV, G.A. and Catenazzi, A. and Whitfield, S.M. and Kurz, D.J. and Suárez-Mayorga, Á. and Aponte-Gutiérrez, A. and Donnelly, M.A. and Todd, B.D.},
	year = {2018},
	keywords = {CTmax, ectotherm, fragmentation, microclimate, Forests, Microclimate, Amphibians, reptile, Biodiversity, habitat loss, Amphibia, animal, Reptiles, Animals, ecosystem, Ecosystem, Reptilia, Squamata, human, Humans, biodiversity, forest, global change, Agriculture, meta analysis (topic), Meta-Analysis as Topic, phylogenetic signal, species traits},
	pages = {345--355},
}

@article{frishkoff_thermal_2015,
	title = {Thermal niche predicts tolerance to habitat conversion in tropical amphibians and reptiles},
	volume = {21},
	issn = {13541013 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84944028464&doi=10.1111%2fgcb.13016&partnerID=40&md5=789c8a42a08e2052fd30a9d44fc9cc56},
	abstract = {Habitat conversion is a major driver of the biodiversity crisis, yet why some species undergo local extinction while others thrive under novel conditions remains unclear. We suggest that focusing on species' niches, rather than traits, may provide the predictive power needed to forecast biodiversity change. We first examine two Neotropical frog congeners with drastically different affinities to deforestation and document how thermal niche explains deforestation tolerance. The more deforestation-tolerant species is associated with warmer macroclimates across Costa Rica, and warmer microclimates within landscapes. Further, in laboratory experiments, the more deforestation-tolerant species has critical thermal limits, and a jumping performance optimum, shifted {\textasciitilde}2 °C warmer than those of the more forest-affiliated species, corresponding to the {\textasciitilde}3 °C difference in daytime maximum temperature that these species experience between habitats. Crucially, neither species strictly specializes on either habitat - instead habitat use is governed by regional environmental temperature. Both species track temperature along an elevational gradient, and shift their habitat use from cooler forest at lower elevations to warmer deforested pastures upslope. To generalize these conclusions, we expand our analysis to the entire mid-elevational herpetological community of southern Costa Rica. We assess the climatological affinities of 33 amphibian and reptile species, showing that across both taxonomic classes, thermal niche predicts presence in deforested habitat as well as or better than many commonly used traits. These data suggest that warm-adapted species carry a significant survival advantage amidst the synergistic impacts of land-use conversion and climate change. © 2015 John Wiley \& Sons Ltd.},
	language = {English},
	number = {11},
	journal = {Global Change Biology},
	author = {Frishkoff, L.O. and Hadly, E.A. and Daily, G.C.},
	year = {2015},
	keywords = {Forests, Temperature, climate change, performance, temperature, Amphibians, amphibian, ecophysiology, reptile, Biodiversity, Ecophysiology, Amphibia, animal, physiology, biological model, Reptiles, Animals, Climate Change, ecosystem, Ecosystem, Reptilia, habitat use, temperature effect, biodiversity, niche, Models, Conservation of Natural Resources, environmental protection, forest, land use change, Leptodactylidae, tropical environment, habitat fragmentation, Land-use change, land-use change, Biological, community ecology, Community ecology, Costa Rica, countryside biogeography, Countryside biogeography, Craugastor, deforestation, Deforestation, Performance},
	pages = {3901--3916},
}

@article{maness_acute_1980,
