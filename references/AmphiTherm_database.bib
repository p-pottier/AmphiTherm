@article{siddons_exposure_2021,
	title = {Exposure to a fungal pathogen increases the critical thermal minimum of two frog species},
	volume = {11},
	copyright = {© 2021 The Authors. Ecology and Evolution published by John Wiley \& Sons Ltd.},
	issn = {2045-7758},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ece3.7779},
	doi = {10.1002/ece3.7779},
	abstract = {The ability of an organism to tolerate seasonal temperature changes, such as extremely cold temperatures during the winter, can be influenced by their pathogens. We tested how exposure to a virulent fungal pathogen, Batrachochytrium dendrobatidis (Bd), affected the critical thermal minimum (CTmin) of two frog species, Hyla versicolor (gray treefrog) and Lithobates palustris (pickerel frog). The CTmin is the minimum thermal performance point of an organism, which we estimated via righting response trials. For both frog species, we compared the righting response of Bd-exposed and Bd-unexposed individuals in either a constant (15ºC) environment or with decreasing temperatures (−1°C/2.5 min) starting from 15°C. The CTmin for both species was higher for Bd-exposed frogs than unexposed frogs, and the CTmin of H. versicolor was higher than L. palustris. We also found that Bd-exposed frogs of both species righted themselves significantly fewer times in both decreasing and constant temperature trials. Our findings show that pathogen exposure can reduce cold tolerance and limit the thermal performance range of hosts, which may lead to increased overwintering mortality.},
	language = {en},
	number = {14},
	urldate = {2025-02-21},
	journal = {Ecology and Evolution},
	author = {Siddons, Spencer R. and Searle, Catherine L.},
	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ece3.7779},
	keywords = {amphibians, Batrachochytrium dendrobatidis, chytrid, chytridiomycosis, Hyla versicolor, Lithobates palustris},
	pages = {9589--9598},
	file = {Full Text PDF:C\:\\Users\\patri\\Zotero\\storage\\R2XFMB8Q\\Siddons and Searle - 2021 - Exposure to a fungal pathogen increases the critical thermal minimum of two frog species.pdf:application/pdf;Snapshot:C\:\\Users\\patri\\Zotero\\storage\\C6XPU9VN\\ece3.html:text/html},
}

@article{swanson_freezing_1996,
	title = {Freezing tolerance/intolerance and cryoprotectant synthesis in terrestrially overwintering anurans in the {Great} {Plains}, {USA}},
	volume = {166},
	issn = {1432-136X},
	url = {https://doi.org/10.1007/BF00301174},
	doi = {10.1007/BF00301174},
	abstract = {Mechanistic bases for freezing tolerance in anurans have been well-studied only in wood frogs, Rana sylvatica, so comprehensive explanations for the mechanisms and evolution of freezing tolerance in anurans are lacking. We measured crystallization temperatures, freezing tolerance/intolerance, and tissue glucose and glycogen phosphorylase activities in frozen and unfrozen winter-acclimated Pseudacris triseriata, Bufo cognatus and B. woodhousei. Freezing occurred at higher subzero temperatures on wet substrate than on dry substrate in all species, indicating susceptibility to inoculative freezing. P. triseriata was freeze-tolerant, but survival was dependent on the level of supercooling prior to freezing. All Bufo were freezing intolerant, regardless of crystallization temperature. Glucose was significantly elevated by freezing in both liver (35-fold) and leg muscle (22-fold) in winter P. triseriata, but only liver glucose was significantly elevated in B. cognatus. However, freezing did not alter glycogen phosphorylase activity in either species. Liver phosphorylase activity was significantly higher in P. triseriata than in B. cognatus, suggesting that capacity for mobilizing glucose from liver glycogen is associated with freezing tolerance. Summer measurements of liver phosphorylase activity, however, did not differ between species. Thus, P. triseriata, but not B. cognatus, exhibited winter increment of liver phosphorylase activity that is correlated with the development of freezing tolerance.},
	language = {en},
	number = {2},
	urldate = {2025-02-21},
	journal = {Journal of Comparative Physiology B},
	author = {Swanson, D. L. and Graves, B. M. and Koster, K. L.},
	month = jun,
	year = {1996},
	keywords = {Overwintering, Glycogen phosphorylase, Freezing tolerance, Supercooling, Anurans},
	pages = {110--119},
	file = {Full Text PDF:C\:\\Users\\patri\\Zotero\\storage\\KXWHKVEM\\Swanson et al. - 1996 - Freezing toleranceintolerance and cryoprotectant synthesis in terrestrially overwintering anurans i.pdf:application/pdf},
