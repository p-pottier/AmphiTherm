@article{catenazzi_thermal_2014,
	title = {Thermal physiology, disease, and amphibian declines on the eastern slopes of the andes},
	volume = {28},
	issn = {08888892 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84896281618&doi=10.1111%2fcobi.12194&partnerID=40&md5=2c6517cfefe0ed0245447daeee93f7fe},
	abstract = {Rising temperatures, a widespread consequence of climate change, have been implicated in enigmatic amphibian declines from habitats with little apparent human impact. The pathogenic fungus Batrachochytrium dendrobatidis (Bd), now widespread in Neotropical mountains, may act in synergy with climate change causing collapse in thermally stressed hosts. We measured the thermal tolerance of frogs along a wide elevational gradient in the Tropical Andes, where frog populations have collapsed. We used the difference between critical thermal maximum and the temperature a frog experiences in nature as a measure of tolerance to high temperatures. Temperature tolerance increased as elevation increased, suggesting that frogs at higher elevations may be less sensitive to rising temperatures. We tested the alternative pathogen optimal growth hypothesis that prevalence of the pathogen should decrease as temperatures fall outside the optimal range of pathogen growth. Our infection-prevalence data supported the pathogen optimal growth hypothesis because we found that prevalence of Bd increased when host temperatures matched its optimal growth range. These findings suggest that rising temperatures may not be the driver of amphibian declines in the eastern slopes of the Andes. Zoonotic outbreaks of Bd are the most parsimonious hypothesis to explain the collapse of montane amphibian faunas; but our results also reveal that lowland tropical amphibians, despite being shielded from Bd by higher temperatures, are vulnerable to climate-warming stress. © 2013 Society for Conservation Biology.},
	language = {English},
	number = {2},
	journal = {Conservation Biology},
	author = {Catenazzi, A. and Lehr, E. and Vredenburg, V.T.},
	year = {2014},
	keywords = {Frogs, Temperature, climate change, temperature, critical thermal maximum, Anura, Biodiversity, frog, animal, physiology, frogs, Animals, Chytridiomycetes, Chytridiomycota, Climate Change, microbiology, Mycoses, prevalence, Prevalence, veterinary, endangered species, physiological response, extinction, Critical thermal maximum, fungus, population decline, temperature effect, biodiversity, species conservation, Extinction, elevation, Conservation of Natural Resources, environmental protection, physiological ecology, disease prevalence, Andes, growth, Physiological ecology, anthropogenic effect, pathogen, development and aging, bosque de montana, bosque de montaña, disease spread, ecologia fisiologica, ecología fisiológica, elevational gradient, Elevational gradient, epidemiology, especies amenazadas, extincion, extinción, gradiente de elevacion, gradiente de elevación, maximo termal critico, máximo termal crítico, montane forest, Montane forest, mountain region, Neotropic Ecozone, Peru, ranas, thermal alteration, threatened species, Threatened species, tropical Andes, Tropical Andes, zona tropical de los Andes},
