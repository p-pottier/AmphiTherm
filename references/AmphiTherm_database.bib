	year = {1988},
	keywords = {Thermoregulation, thermoregulation, Anura, Body Temperature Regulation, frog, Amphibia, arid, Estivation, Hyperoliidae, Hyperolius, Hyperolius viridiflavus, Phragmites, reed frog, water use, Water-balance},
	pages = {327--338},
}

