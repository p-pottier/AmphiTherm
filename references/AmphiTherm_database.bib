@article{sanabria_thermal_2015,
	title = {Thermal ecology of the post-metamorphic {Andean} toad ({Rhinella} spinulosa) at elevation in the monte desert, {Argentina}},
	volume = {52},
	issn = {03064565 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929622396&doi=10.1016%2fj.jtherbio.2015.05.006&partnerID=40&md5=4d17b74eb10dc9ab4b7597ec0c89e83f},
	abstract = {Rhinella spinulosa is an anuran toad species distributed latitudinal and altitudinal (1200-5000. m) from Peru to Argentina, inhabiting mountain valleys in the Andes. Considering the broad range of habitats where they live, it is important to understand the thermal physiological mechanisms, thermal tolerances and physiological adaptations for surviving in rigorous environments. We investigated the thermal parameters (field body temperature, selected body temperature, locomotor performance in field and laboratory conditions, and thermal extremes) during diurnal activity for a population of juvenile, post-metamorphosed toads (Rhinella spinulosa) from the Monte Desert of San Juan, Argentina. Post-metamorphic toads are active from approximately 1100-1900 (in contrast to nocturnal adult toads). Our findings show that these toads have a wide thermal tolerance range, ranging from a critical thermal maximum of 36.9. °C to crystallization temperatures below 0. °C. During their active period, toads always showed suboptimal thermal conditions for locomotion. Despite the suboptimal condition for the locomotion, diurnal activity is likely to confer thermal advantages, allowing them to search for food and increase digestion and growth rates. We also found that the toads are capable of super-cooling, which prevents mortality from freezing when the environmental temperatures drop below 0. °C. The environmental temperatures are below zero at night, when toads are inactive and take refuge under rocks. In summary, this toad population demonstrates high thermal plasticity, as shown by a relatively high level of activity sustained over a wide range of ambient temperature ({\textasciitilde}35 °C). These thermal adaptations allow this species of juvenile toads to inhabit a wide range of altitudes and latitudes. © 2015.},
	language = {English},
	journal = {Journal of Thermal Biology},
	author = {Sanabria, E.A. and Rodríguez, C.Y. and Vergara, C. and Ontivero, E. and Banchig, M. and Navas, A.L. and Herrera-Morata, M.A. and Quiroga, L.B.},
	year = {2015},
	keywords = {Temperature, temperature, body temperature, critical thermal maximum, Anura, Ecology, phenotypic plasticity, Altitude, Body Temperature, Acclimatization, animal, acclimatization, ecology, physiology, animal experiment, temperature acclimatization, nonhuman, larva, Larva, Bufonidae, Animals, controlled study, Survival Analysis, locomotion, Critical thermal maximum, supercooling, altitude, environmental temperature, motor activity, Motor Activity, Supercooling, low temperature, high temperature, aging, metamorphosis, Metamorphosis, Argentina, humidity, Humidity, adult, Article, Locomotor performance, body temperature measurement, survival analysis, feeding behavior, Feeding Behavior, Field body temperature, Aging, crystallization temperature, Crystallization temperature, diurnal animal, field body temperature, induced hypothermia, juvenile animal, Rhinella spinulosa, selected body temperature, Biological},
	pages = {52--57},
}

@article{sherman_thermal_2008,
	title = {Thermal biology of newts ({Notophthalmus} viridescens) chronically infected with a naturally occurring pathogen},
