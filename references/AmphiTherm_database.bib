	note = {Publisher: NRC Research Press},
	pages = {1230--1235},
	file = {Full Text PDF:C\:\\Users\\patri\\Zotero\\storage\\L7XCH6GA\\Galindo et al. - 2018 - Evaluation of the combined temperature and relative humidity preferences of the Colombian terrestria.pdf:application/pdf},
}

